{
  "name": "create-stb",
  "version": "1.0.0",
  "description": "create-stb boilerplate template",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf .build .serverless",
    "deploy": "serverless deploy",
    "dynamo:destroy": "docker compose down -v",
    "dynamo:down": "docker compose down",
    "dynamo:up": "docker compose up -d",
    "format": "prettier --write .",
    "offline": "serverless offline --stage dev",
    "tables:create": "ts-node src/scripts/create-tables.ts",
    "tables:seed": "ts-node src/scripts/seed-tables.ts"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "3.946.0",
    "@aws-sdk/util-dynamodb": "3.946.0",
    "axios": "1.13.2",
    "dotenv": "17.2.3"
  },
  "devDependencies": {
    "@types/aws-lambda": "8.10.159",
    "@types/dotenv": "8.2.3",
    "@types/node": "24.10.1",
    "prettier": "3.7.4",
    "serverless": "4.27.0",
    "serverless-offline": "14.4.0",
    "typescript": "5.9.3"
  },
  "keywords": [
    "create-stb"
  ]
}
